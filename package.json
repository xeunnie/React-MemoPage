{
  "name": "iuc-react-typescript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "react-scripts start",
    "prestart:prod": "yarn run build",
    "start:prod": "react-scripts start",
    "prebuild": "rimraf build",
    "build": "react-scripts build",
    "postinstall": "git config --local core.hooksPath ./.git-hooks",
    "eslint": "eslint --max-warnings 0 $(git diff --name-only --diff-filter=AMRC HEAD | grep -E '.(ts|tsx)$' | xargs)"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "sass": "^1.54.8"
  },
  "devDependencies": {
    "@types/node": "^16.11.56",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-name": "^1.2.3",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "react-scripts": "5.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-npm"
  }
}
